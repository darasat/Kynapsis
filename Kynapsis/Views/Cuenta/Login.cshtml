<body>
    <div class="container">
        <form id="Form1" class="form-login" action="index.html">
            <h2 class="form-login-heading">Iniciar cuenta</h2>
            <div class="login-wrap">
                <div class="form-group">
                    <label for="inputName" class="control-label">Nombre</label>
                    <input type="text" class="form-control" id="inputName" placeholder="nombre" required>
                </div>
                <div class="form-group" id="divPass">
                    <label for="inputPassword" class="control-label">Contraseña</label>
                    <input type="password" data-minlength="6" class="form-control" id="inputPassword" placeholder="contraseña" required>
                    <div class="help-block">Minimo 6 Caracteres</div>
                </div>
                <div class="form-group">
                    <label class="checkbox">
                        <span class="pull-right">
                            <a href="olvide mi contraseña.html">¿Olvidó su contraseña?</a>
                        </span>
                    </label>
                </div>
                <div class="form-group">
                    <button type="submit" class="btn-theme btn-block btn btn-primary">Ingresar</button>
                </div>
                <div class="form-group">
                    <div class="registration">
                        ¿Ya estas inscrito?<br />
                        <a href="inscripcion.html">
                            Crear cuenta
                        </a>
                    </div>
                </div>
            </div>
        </form>
    </div>
    <br><br><br><br><br><br><br>
    <footer class="site-footer"></footer>
</body>

